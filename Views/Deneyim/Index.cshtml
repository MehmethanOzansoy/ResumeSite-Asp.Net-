@using MVCCv.Models.Entity
@model List<TblDeneyimlerim>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />




<h2 class="display-3 text-center text-capitalize mx-md-3">Deneyim Listesi</h2>

<table class="table table-bordered">
    <tbody>
        <tr>
            <th>ID</th>
            <th>Başlık</th>
            <th>Alt Başlık</th>
            <th>Tarih</th>
            <th>Sil</th>
            <th>Düzenle</th>

        </tr>
        @foreach (var a in Model)
        {
            <tr>
                <td>@a.ID</td>
                <td>@a.Baslik</td>
                <td>@a.AltBaslik</td>
                <td>@a.Tarih</td>
                <td><a href="/Deneyim/DeneyimSil/@a.ID" class="btn btn-danger">Sil</a></td>
                <td><a href="/Deneyim/DeneyimGuncelle/@a.ID" class="btn btn-warning">Düzenle</a></td>
            </tr>
        }
    </tbody>
</table>

<a href="/Deneyim/DeneyimEkle/" class="btn btn-primary">Yeni Deneyim Ekle</a>