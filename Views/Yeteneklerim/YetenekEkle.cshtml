
@{
    ViewBag.Title = "YetenekEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1 style="text-align:center">Yetenek Ekleme Sayfası</h1>
<div class="card card-info">
    <div class="card-header">
        <h3 class="card-title">Horizontal Form</h3>
    </div>
    <form class="form-horizontal" method="post">
        <div class="card-body">
            <div class="form-group row">
                <label for="yetenek" class="col-sm-2 col-form-label">Yetenek</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" placeholder="Yetenek Giriniz" name="Yetenek">
                </div>
            </div>
            <div class="form-group row">
                <label for="oran" class="col-sm-2 col-form-label">Oran</label>
                <div class="col-sm-10">
                    <input type="number" class="form-control" placeholder="Oran Giriniz" name="Oran">
                </div>
            </div>
        </div>
        <!-- /.card-body -->
        <div class="card-footer">
            <button type="submit" class="btn btn-info">Onayla</button>
        </div>
        <!-- /.card-footer -->
    </form>


</div>


